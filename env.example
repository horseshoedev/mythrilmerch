# MythrilMerch Environment Variables
# Copy this file to .env and fill in your actual values

# Neon Database URL (Primary - for production)
NETLIFY_DB_URL=postgresql://username:password@hostname/database

# Local Database (Fallback - for development)
DB_HOST=localhost
DB_NAME=ecommerce_db
DB_USER=ecommerce_user
DB_PASSWORD=your_strong_password

# Flask Configuration
FLASK_DEBUG=0 